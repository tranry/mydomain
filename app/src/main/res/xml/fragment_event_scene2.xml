            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="70dp"
            android:layout_width="wrap_content"
            android:id="@+id/textView9" />
        <Constraint
            motion:layout_editor_absoluteY="-3dp"
            android:layout_height="264dp"
            motion:layout_editor_absoluteX="6dp"
            android:layout_width="410dp"
            android:id="@+id/imgCheckDomain" />
        <Constraint
            motion:layout_editor_absoluteY="531dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="211dp"
            android:layout_width="wrap_content"
            android:id="@+id/textKQ" />
